{
  "name": "notebook",
  "version": "0.1.0",
  "main": "index.js",
  "homepage": "https://yszk0123.github.io/notebook",
  "repository": "ssh://git@github.com/yszk0123/notebook.git",
  "author": "Yuji Suzuki <y0suzuki0123@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel ./src/index.pug --no-hmr -p 3000",
    "prebuild": "rm -rf dist",
    "build": "run-p build:*",
    "build:app": "cross-env NODE_ENV=production parcel build ./src/index.pug --public-url $APP_BASE_URL",
    "build:404": "cross-env NODE_ENV=production parcel build 404.pug --public-url $APP_BASE_URL",
    "predeploy": "yarn build",
    "deploy": "gh-pages -m 'auto commit [ci skip]' -d dist",
    "deploy:ci": "gh-pages -m 'auto commit [ci skip]' --repo https://$GH_TOKEN@github.com/yszk0123/notebook.git -d dist",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "run-p lint:*",
    "lint:typecheck": "tsc --noEmit",
    "lint:tslint": "tslint -p tsconfig.json"
  },
  "devDependencies": {
    "@types/history": "4.7.2",
    "@types/lodash": "4.14.133",
    "@types/node": "11.13.13",
    "@types/react": "16.8.19",
    "@types/react-datepicker": "2.3.0",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.0.9",
    "@types/styled-components": "4.1.15",
    "@types/universal-router": "8.0.0",
    "cross-env": "5.2.0",
    "gh-pages": "2.0.1",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-sw-precache": "1.1.1",
    "prettier": "1.17.1",
    "pug": "2.0.3",
    "redux-logger": "3.0.6",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "date-fns": "^1.29.0",
    "firebase": "^5.9.3",
    "firebaseui": "^3.5.1",
    "history": "^4.9.0",
    "immutability-helper": "^3.0.0",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "option-t": "^18.0.1",
    "react": "^16.8.6",
    "react-datepicker": "^2.3.0",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^4.2.0",
    "universal-router": "^8.1.0"
  },
  "sw-precache": {
    "maximumFileSizeToCacheInBytes": 10485760
  }
}
