  "scripts": {
    "start": "cross-env NODE_ENV=development parcel ./src/index.pug -p 3000",
    "start:editor-example": "cross-env NODE_ENV=development parcel editor-example.html -p 3000",
    "prebuild": "rm -rf dist",
    "build": "run-p build:*",
    "build:app": "cross-env NODE_ENV=production parcel build ./src/index.pug --public-url $APP_BASE_URL",
    "build:404": "cross-env NODE_ENV=production parcel build 404.pug --public-url $APP_BASE_URL",
    "predeploy": "yarn build",
    "deploy": "gh-pages -m 'auto commit [ci skip]' -d dist",
    "deploy:ci": "gh-pages -m 'auto commit [ci skip]' --repo https://$GH_TOKEN@github.com/yszk0123/notebook.git -d dist",
    "lint": "run-p lint:*",
    "lint:typecheck": "tsc --noEmit",
    "lint:tslint": "tslint -p tsconfig.json"
  },
